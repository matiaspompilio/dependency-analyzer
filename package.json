{
  "name": "dependency-analyzer",
  "version": "0.1.0",
  "main": "app.js",
  "author": "matiaspompilio",
  "license": "MIT",
  "scripts": {
    "analyze": "node app.js",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.{js,json,md}\" && eslint --fix . --ext .js",
    "test": "jest"
  },
  "bin": {
    "deps-analyze": "./app.js",
    "deps": "./app.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "commander": "^7.1.0",
    "csv-parser": "^3.0.0",
    "path": "^0.12.7",
    "untildify": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
