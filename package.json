{
  "name": "dependency-analyzer",
  "version": "1.0.0",
  "main": "app.js",
  "author": "matias pompilio",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/app.js",
    "lint": "eslint --ext .js ."
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": "eslint --cache --fix"
  }
}